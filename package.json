    "hardhat-deploy": "0.11.19",
    "hardhat-docgen": "1.3.0",
    "hardhat-gas-reporter": "1.0.9",
    "hardhat-tracer": "3.4.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "npm-run-all2": "6.2.0",
    "prettier": "2.8.8",
    "prettier-plugin-solidity": "1.0.0-beta.24",
    "solhint": "3.3.7",
    "solidity-bytes-utils": "0.8.4",
    "solidity-coverage": "0.8.16",
    "solidity-docgen": "0.6.0-beta.35",
    "ts-morph": "26.0.0",
    "ts-node": "10.9.1",
    "typechain": "8.1.0",
    "typescript": "4.8.4",
    "typescript-eslint": "8.46.4"
  },
  "resolutions": {
    "@layerzerolabs/oft-evm/@openzeppelin/contracts": "4.8.3"
  },
  "packageManager": "yarn@4.10.3",
  "lint-staged": {
    "*.{js,ts}": [
      "yarn lint:ts:fix ",
      "yarn format:ts:fix"
    ],
    "*.{json,yml,yaml}": [
      "yarn format:yml:fix"
    ],
    "*.sol": [
      "yarn lint:sol:fix",
      "yarn format:sol:fix"
    ]
  }
